        "prop-types": "^15.8.1",
        "react": "^18.0.0",
        "react-dom": "^18.0.0",
        "react-hook-form": "^7.33.1",
        "react-router-dom": "^6.3.0",
        "swr": "^1.3.0",
        "vite-plugin-imp": "^2.2.0",
        "vite-plugin-svgr": "^2.2.0",
